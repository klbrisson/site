{
  "name": "computer-history-timeline",
  "version": "1.0.0",
  "description": "A timeline of the last 40 years of computer history.",
  "engines": {
    "node": "6.4.0"
  },
  "scripts": {
    "build": "npm run lint && tsc && npm run sass-build",
    "start": "\"lite-server\"",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install && tsc",
    "sass-build": "sass --update app/resources/css:app/resources/css",
    "sass:w": "sass --watch app/resources/css:app/resources/css",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "bower": "1.7.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.0",
    "express": "^4.14.0",
    "jquery": "^3.1.0",
    "mongodb": "^2.2.9",
    "mongoose": "^4.5.10",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.2.0",
    "serve-favicon": "^2.3.0",
    "systemjs": "0.19.27",
    "typescript": "^2.2.2",
    "typings": "^2.1.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "codelyzer": "^3.0.0-beta.4",
    "concurrently": "^2.2.0",
    "gem": "^2.1.5",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "lite-server": "^2.2.0",
    "lodash": "^4.11.1",
    "proto": "^1.0.19",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.2",
    "ruby": "^0.6.1",
    "sass": "^0.5.0",
    "tslint": "^4.5.1",
    "typescript": "^1.8.10",
    "typings": "^2.1.0"
  },
  "repository": {}
}
